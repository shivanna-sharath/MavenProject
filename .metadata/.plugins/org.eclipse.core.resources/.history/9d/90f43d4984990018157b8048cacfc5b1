class firstc extends Thread{
	public void run() {
		int fir=0;
		int sec=1;
		int sum;
		for(int i=0;i<10;i++) {
		sum=fir+sec;
		fir=sec;
		sec=sum;
		System.out.println( sum);
		}
		
		
	}
}
class seconds extends Thread{
	int n;
	public void run() {
	for(int j=1;j<10;j++) {
		n=j;
	
		int result=1;
		for(int i=n;i>0;i--) {
			result=result*i;
		}

		System.out.println(n  +"    "  +result);
	}
	}
	
}





public class N {

	public static void main(String[] args) {
		firstc a=new firstc();
		seconds b=new seconds();
			a.start();
			a.join();
			b.start();
			}
		}
                                                                                          



