import java.util.ArrayList;
class first extends Thread{
	public void run() {
		int a=0;
		int b=1;
		int c;
		for (int i=0;i<10;i++) {
			c=a+b;
			if(i%2==1) {
				try {
					Thread.sleep(2000);
				} catch (InterruptedException e) {
					e.printStackTrace();
				}
				
			}
			else {
			
			System.out.println(c);
			
			}
			a=b;
			b=c;
		}
	}
	
}
class second extends Thread{
	public void run() {
		int a=0;
		int b=1;
		int c;
		
		for (int i=0;i<10;i++) {
			c=a+b;
			if(i%2==0) {
				try {
					Thread.sleep(2000);
				} catch (InterruptedException e) {

					e.printStackTrace();
				}
			}
			else {
			
			System.out.println(c);
		
	}
			a=b;
			b=c;
		}
}
}




public class D {

	public static void main(String[] args) throws InterruptedException {
		first a=new first();
		second b=new second();
		a.start();
		Thread.sleep(10);
		b.start();
		
	
		
		


	}
}
	
	

