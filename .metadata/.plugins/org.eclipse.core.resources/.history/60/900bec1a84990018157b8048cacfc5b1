class firstc extends Thread{
	public void run() {
		int fir=0;
		int sec=1;
		int sum;
		for(int i=0;i<10;i++) {
		sum=fir+sec;
		fir=sec;
		sec=sum;
		}
		
		
	}
}
class seconds extends Thread{
	public void run() {
		int n=5;
		int result=1;
		for(int i=n;i>0;i--) {
			result=result*i;
		}
		
	}
	
}




public class N {

	public static void main(String[] args) {
		firstc a=new firstc();
		seconds b=new seconds();
			
			}
		}




