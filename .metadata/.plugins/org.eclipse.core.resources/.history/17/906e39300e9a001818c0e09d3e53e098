package collections1;
class first extends Thread{
	public void run() {
		for( int i=0;i<10;i++) {
			if(i%2==0) {
				System.out.println(i);
			}
		}
	}
	
}
class second extends Thread{
public void run() {
	for(int i=0;i<10;i++) {
		if(i%2!=0) {
			System.out.println(i);
		}
		
	}
	
}
}

public class Threadexample {

	public static void main(String[] args) throws InterruptedException {
		first a=new first();
		second b=new second();
		a.start();
		a.join();
		b.start();
		

	}

}
